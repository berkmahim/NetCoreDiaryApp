@model List<DiaryEntry>

<div class="container">
    <div class="row pt-4">
        <div class="col-6">
            <a href="" class="btn btn-outline-primary" style="margin-bottom: 50px">
                Create New Diary
            </a>
        </div>
        <div class="col-6 text-end">
            Diaeries
        </div>
    </div>
</div>



<div class="table-responsive">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
        <th>Title</th>    
        <th>Content</th>    
        <th>Date</th>    
        <th>Actions</th>  
        </tr>
        </thead>
        
        
        <tbody>
        @foreach (var entry in Model)
        {
            <tr>
                <td>@entry.Title</td>
                <td>@entry.Content</td>
                <td>@entry.Created</td>
                <td><button type="button" class="btn btn-outline-primary">Edit</button></td>
                <td><button type="button" class="btn btn-outline-danger">Delete</button></td>
            </tr>
 
        }

        </tbody>
    </table>
    
    
    
</div>